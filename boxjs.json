{
  "id": "fuba_sign",
  "name": "福利吧签到", 
  "description": "福利吧每日自动签到脚本，兼容BoxJs以及Quantumult X",
  "author": "@ader90520",
  "icon": "https://raw.githubusercontent.com/leiyiyan/resource/main/icons/pyy.png",
  "repo": "https://github.com/ader90520/fulibaqiandao",
  "apps": [
    {
      "id": "fuba_sign",
      "name": "福利吧签到",
      "author": "@ader90520", 
      "repo": "https://github.com/ader90520/fulibaqiandao",
      "icons": [
        "https://raw.githubusercontent.com/leiyiyan/resource/main/icons/pyy.png",
        "https://raw.githubusercontent.com/leiyiyan/resource/main/icons/pyy.png"
      ],
      "keys": [
        "fuba_cookie", 
        "fuba_username",
        "fuba_domain"
      ],
      "settings": [
        {
          "id": "fuba_domain",
          "name": "福利吧域名",
          "val": "www.wnflb2023.com",
          "type": "select",
          "options": [
            {
              "name": "wnflb2023.com",
              "val": "www.wnflb2023.com"
            },
            {
              "name": "wnflb00.com", 
              "val": "www.wnflb00.com"
            },
            {
              "name": "wnflb99.com",
              "val": "www.wnflb99.com"
            }
          ],
          "desc": "选择福利吧访问域名"
        },
        {
          "id": "fuba_cookie",
          "name": "福利吧Cookie", 
          "val": "",
          "type": "textarea",
          "autoGrow": true,
          "placeholder": "请输入登录福利吧后获取的Cookie值",
          "desc": "获取方法：登录福利吧后按F12打开开发者工具，复制Network请求中的Cookie值[citation:4]"
        },
        {
          "id": "fuba_username",
          "name": "福利吧用户名",
          "val": "", 
          "type": "text",
          "placeholder": "请输入您的福利吧登录用户名",
          "desc": "用于验证Cookie有效性"
        }
      ],
      "scripts": [
        {
          "name": "福利吧签到",
          "description": "每日自动签到福利吧，获取积分", 
          "cron": "30 8 * * *"
        }
      ],
      "descs_html": [
        "<p style='margin-left:20px;'>福利吧自动签到脚本</p >",
        "<p style='margin-left:20px; color: orange;'>请确保Cookie和用户名填写正确，否则会导致签到失败</p >",
        "<p style='margin-left:20px;'>使用步骤：</p >",
        "<ol style='margin-left:40px;'>",
        "<li>登录福利吧网站</li>", 
        "<li>按F12打开开发者工具[citation:4]</li>",
        "<li>复制Network请求中的Cookie</li>",
        "<li>在此处填写Cookie和用户名</li>",
        "</ol>"
      ]
    }
  ]
}
